{
  "name": "@duncte123/obs-websocket-js",
  "version": "4.9.2",
  "description": "OBS Websocket API in Javascript, consumes @Palakis/obs-websocket",
  "author": "duncte123",
  "license": "MIT",
  "repository": "duncte123/obs-websocket-js",
  "repoUrl": "https://github.com/duncte123/obs-websocket-js",
  "keywords": [
    "obs",
    "studio",
    "websocket",
    "node",
    "node.js"
  ],
  "main": "dist/index.js",
  "browser": "browser-dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "browser-dist"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "prebuild:types": "eslint .",
    "build:types": "cross-env-shell TS_NODE_PROJECT=tsconfig.json \"node $NODE_DEBUG_OPTION -r ts-node/register scripts/build-types2.ts\"",
    "build:ts": "tsc",
    "build:web": "rimraf browser-dist && webpack",
    "watch": "webpack --watch",
    "debug:test": "cross-env DEBUG=obs-websocket-js:* npm run test",
    "test": "npm-run-all test:*",
    "test:static": "eslint .",
    "test:ava": "npm run ava",
    "skip-test:types": "dtslint test/type-tests.ts",
    "report": "nyc report --reporter=text-lcov",
    "node-coveralls": "npm run report | coveralls",
    "ava": "nyc ava --verbose",
    "static": "eslint ."
  },
  "dependencies": {
    "debug": "^4.1.0",
    "isomorphic-ws": "^4.0.1",
    "sha.js": "^2.4.9",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/got": "^9.6.9",
    "@types/lodash.deburr": "^4.1.6",
    "@types/node": "^10.17.6",
    "@types/node-fetch": "^2.5.10",
    "@types/prettier": "^1.19.0",
    "@types/sha.js": "^2.4.0",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "ava": "^3.2.0",
    "babili-webpack-plugin": "^0.1.2",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "dtslint": "^4.1.0",
    "eslint": "^7.28.0",
    "eslint-plugin-ava": "^12.0.0",
    "lodash.deburr": "^4.1.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^4.2.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "bugs": {
    "url": "https://github.com/duncte123/obs-websocket-js/issues"
  },
  "homepage": "https://github.com/duncte123/obs-websocket-js#readme",
  "ava": {
    "files": [
      "test/*.spec.js",
      "!setup/environment.js"
    ],
    "concurrency": 1,
    "timeout": "10s",
    "verbose": true
  }
}
